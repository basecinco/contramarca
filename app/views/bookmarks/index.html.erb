<div class="container">
  <h2 class="header">Bookmarks</h2>
  <br>
  <% if @my_saved_stories.count == 0 %>
  <br><br>
  <h2 class="header">You have no saved stories yet</h2>
  <% else %>
  <% @my_saved_stories.each do |s| %>
    <%=render 'shared/smallcards', s: s %>
    <br>
  <% end %>
  <% end %>

</div>


