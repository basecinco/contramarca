
<div class="container">
  <h1>Stories#show</h1>
  <div>
  <%= @story.user_id %>
  <h1><%= @story.title %></h1>
  <%= @story.created_at.strftime("%B %e, %Y") %>
  <%= @story.content %>
  <%= link_to 'Back', stories_path %>
    <% @story.images.each do |img| %>
    <%= cl_image_tag img.data %>
  <% end %>
  </div>


  <%= render 'shared/story_compact', story: @story  %>
</div>

